{
  "name": "@newkind/events",
  "version": "0.2.5",
  "description": "task manager for async events",
  "main": "./dist/index.mjs",
  "author": "Zababurin Sergey",
  "license": "GPL-3.0-only",
  "config": {
    "port": 3269
  },
  "scripts": {
    "start": "start",
    "dev": "parcel ./src/index.html -p $npm_package_config_port --cache-dir ./.parcel-cache",
    "build": "parcel build ./src/index.mjs --cache-dir ./.parcel-cache",
    "kill": "kill $(lsof -t -i:$npm_package_config_port)",
    "clean": "rimraf ../../.parcel-cache"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "npm-run-all": "^4.1.5",
    "parcel": "2.0.0-rc.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zababurinsv/newkind-events.git"
  },
  "bugs": {
    "url": "https://github.com/zababurinsv/newkind-events/issues"
  },
  "homepage": "https://github.com/zababurinsv/newkind-events#readme"
}
